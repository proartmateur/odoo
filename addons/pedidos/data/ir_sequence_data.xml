<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for lead mining requests -->
        <record id="ir_sequence_pedidos_mine" model="ir.sequence">
            <field name="name">Pedidos Request</field>
            <field name="code">pedidos.request</field>
            <field name="prefix">PED/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="group_is_pedidos" model="res.groups">
            <field name="name">Pedidos Manage</field>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record>
    </data>
</odoo>
